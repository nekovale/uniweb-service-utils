{
  "name": "@nekovale/uniweb-service-utils",
  "version": "1.0.12",
  "description": "uniweb service utils",
  "author": "Nekovale",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://github.com/nekovale/uniweb-service-utils.git"
  },
  "types": "build/lib/index.d.ts",
  "main": "build/lib/index.js",
  "module": "build/lib/index.esm.js",
  "exports": {
    ".": {
      "types": "./build/lib/index.d.ts",
      "import": "./build/lib/index.mjs",
      "default": "./build/lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "build/lib/*",
    "build/umd/*",
    "src"
  ],
  "scripts": {
    "build": "tsc --build && npx rollup -c rollup.config.ts --configPlugin typescript",
    "publish-patch": "yarn build && npm version patch && npm publish"
  },
  "dependencies": {
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.11",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/axios": "^0.14.0",
    "rollup": "^3.28.1",
    "rollup-plugin-preserve-directives": "^0.2.0",
    "rollup-plugin-size": "^0.3.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6"
  }
}
